{
    "name": "manhattan/console-bundle",
    "type": "symfony-bundle",
    "description": "Easy integration of console system for Manhattan Console in Symfony2",
    "keywords": ["security", "layout"],
    "license": "MIT",
    "version": "1.0",
    "authors": [
        {
            "name": "James Rickard",
            "email": "james@frodosghost.com",
            "homepage": "www.frodosghost.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle":            "2.*",
        "symfony/form":                        "2.*",

        "doctrine/doctrine-migrations-bundle": "*",
        "stof/doctrine-extensions-bundle":     "*",
        "stfalcon/tinymce-bundle":             "*",

        "mopa/bootstrap-bundle":               "dev-master",
        "twitter/bootstrap":                   "master",
        "knplabs/knp-paginator-bundle":        "2.*",
        "knplabs/knp-menu-bundle":             "dev-master",
        "craue/formflow-bundle":               "dev-master",

        "behat/behat-bundle":                  "*",
        "behat/mink-bundle":                   "*"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "version": "master",
                "name": "twitter/bootstrap",
                "source": {
                    "url": "https://github.com/twitter/bootstrap.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ]
    "autoload": {
        "psr-0": {
            "Manhattan\\ConsoleBundle": ""
        }
    },
    "scripts": {
        "post-install-cmd": [
            "Mopa\\Bundle\\BootstrapBundle\\Composer\\ScriptHandler::postInstallSymlinkTwitterBootstrap"
        ],
        "post-update-cmd": [
            "Mopa\\Bundle\\BootstrapBundle\\Composer\\ScriptHandler::postInstallSymlinkTwitterBootstrap"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web"
    },
    "target-dir": "Manhattan/ConsoleBundle"
}
